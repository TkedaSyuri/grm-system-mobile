{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASEURL": "https://dev.example.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASEURL": "https://preview.example.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_BASEURL": "https://grm-moniter-mobile-api.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
